{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "noEmit": false,
    "preserveWatchOutput": true,
    "emitDeclarationOnly": false,
    "importHelpers": false,
    "strict": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": false,
    "resolveJsonModule": true
  }
}
